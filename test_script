
/* Programa de ejemplo que suma matrices */

start
	matrix m1 = [{1,2},{3,4}];
	matrix m2 = [{4,5},{6,7}];

	matrix m3 = m1 + m2;
	matrix m4 = m1 * m2; 

	print "m1: ";
	print m1;

	print "\n";

	print "m2: ";
	print m2;

	print "\n";

	print "suma de m1 y m2: ";
	print m3;

	print "\n";

	print "multiplicacion de m1 y m2: ";
	print m4;
end